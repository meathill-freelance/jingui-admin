{
  "name": "21-day-exercise",
  "version": "0.1.0",
  "description": "21 Days Exercise Admin Panel",
  "author": "Meathill <meathill@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --hot --host=localhost --config webpack.config.prod.js",
    "start": "node build/dev-server.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webapck.config.prod.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "stylus": "stylus -m --include-css styl/screen.styl -o static/css",
    "watch:webpack": "webpack",
    "watch:stylus": "stylus -w -m --include-css styl/screen.styl -o static/css/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "lodash": "^4.17.10",
    "moment": "^2.24.0",
    "simple-line-icons": "^2.4.1",
    "vue": "^2.6.10",
    "vue-chartjs": "^2.8.7",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^2.1.1",
    "del": "^3.0.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "0.11.1",
    "friendly-errors-webpack-plugin": "1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "lolex": "^1.5.2",
    "opn": "^4.0.2",
    "ora": "1.2.0",
    "pug": "^2.0.3",
    "rimraf": "^2.6.3",
    "semver": "^5.7.0",
    "shelljs": "^0.7.8",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.19.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-server": "^3.6.0",
    "webpack-merge": "4.1.0"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  }
}
