{
  "name": "21-day-exercise",
  "version": "0.1.0",
  "description": "21 Days Exercise Admin Panel",
  "author": "Meathill <meathill@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --hot --host=localhost --config webpack.config.prod.js",
    "start": "node build/dev-server.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webapck.config.prod.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "stylus": "stylus -m --include-css styl/screen.styl -o static/css",
    "watch:webpack": "webpack",
    "watch:stylus": "stylus -w -m --include-css styl/screen.styl -o static/css/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.7.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "simple-line-icons": "^2.4.1",
    "stylus": "^0.54.5",
    "vue": "^2.6.7",
    "vue-chartjs": "^2.8.7",
    "vue-router": "^3.0.2",
    "vue-strap": "github:wffranco/vue-strap",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "0.11.1",
    "friendly-errors-webpack-plugin": "1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "lolex": "^1.5.2",
    "opn": "^4.0.2",
    "ora": "1.2.0",
    "pug": "^2.0.3",
    "rimraf": "^2.6.3",
    "semver": "^5.6.0",
    "shelljs": "^0.7.8",
    "style-loader": "^0.19.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.2",
    "vue-loader": "^11.3.4",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.6.7",
    "webpack": "^4.29.5"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  }
}
